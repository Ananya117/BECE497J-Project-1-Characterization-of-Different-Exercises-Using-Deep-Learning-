df['lm11_y_filt_v'] = (
    -df['lm11_y_filt'].shift(-2) + 8*df['lm11_y_filt'].shift(-1)
    -8*df['lm11_y_filt'].shift(1) + df['lm11_y_filt'].shift(2)
