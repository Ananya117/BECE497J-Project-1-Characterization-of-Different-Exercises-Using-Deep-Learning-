def fft_lowpass_filter(signal, cutoff_hz, fps):
    N = len(signal)
    fft_vals = np.fft.fft(signal)
    fft_freq = np.fft.fftfreq(N, d=1.0/fps)
    fft_vals[np.abs(fft_freq) > cutoff_hz] = 0
    filtered = np.fft.ifft(fft_vals)
    return np.real(filtered)

